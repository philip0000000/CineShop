@model IReadOnlyList<CineShop.Models.Order>

<h2>Admin Orders (count: @Model.Count)</h2>

<ul>
@foreach (var o in Model) {
  <li>Order #@o.Id — @o.OrderDate — Customer: @o.Customer?.EmailAddress — Rows: @o.OrderRows?.Count</li>
}
</ul>
